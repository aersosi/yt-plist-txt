{
  "manifest_version": 3,
  "name": "YouTube Playlist URL Extractor",
  "version": "1.0",
  "description": "Extracts all video URLs from a YouTube playlist.",
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
